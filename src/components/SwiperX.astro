    ><Image
      src="/imgV/MO/MO 2.jpg"
      width="100"
      height="100"
      class="w-full h-full"
      alt="multimedia"
    /></swiper-slide
  >
  <swiper-slide
    ><Image
      src="/imgV/MO/MO 3.jpg"
      width="100"
      height="100"
      class="w-full h-full"
      alt="multimedia"
    /></swiper-slide
  >

  <swiper-slide
  ><Image
    src="/imgV/MO/MO 53.jpg"
    width="100"
    height="100"
    class="w-full h-full"
    alt="multimedia"
  /></swiper-slide
>
</swiper-container>

<script
  src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-element-bundle.min.js"
></script>
